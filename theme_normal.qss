/*QTimeEdit |  Date Time Edi | Date Edit*/

QSpinBox, QDateTimeEdit, QDateEdit{
	min-height:22px;
	border-radius: 2px;
	}
QSpinBox:focus, QDateTimeEdit:focus,QDateEdit:focus{
	border:1px solid #76ABD7;
}
 QTimeEdit::up-button, QSpinBox::up-button, QDateTimeEdit::up-button {
     width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */    
	 border-left:1px solid #cac6c6;
	 height: 11px;
	 border-top-right-radius: 2px;
	 image: url(:/images/SpinBox_Uparrow_on.png);
	background: qlineargradient(spread:pad, x1:0.488, y1:0.994318, x2:0.455, y2:0, stop:0 rgba(200, 200, 200, 245), stop:1 rgba(226, 226, 226, 255));
	}

 QTimeEdit::up-button:pressed, QSpinBox::up-button:pressed, QDateTimeEdit::up-button:pressed {
	 background-color: #ffffff;
	}
	
QTimeEdit::up-button:disabled, QSpinBox::up-button:disabled, QDateTimeEdit::up-button:disabled {
	background-color: #7c7c7c;
	border:1px solid #4e4e4e;
	}
	
QTimeEdit::down-button, QSpinBox::down-button, QDateTimeEdit::down-button {
     width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
	 background-color: #367ba4;
	 border-left:1px solid #cac6c6;
	 height: 10px;
     border-bottom-right-radius: 2px;
	 border-top:1px solid #cac6c6;
	 image: url(:/images/SpinBox_Downarrow_on.png);
	background: qlineargradient(spread:pad, x1:0.488, y1:0.994318, x2:0.455, y2:0, stop:0 rgba(200, 200, 200, 245), stop:1 rgba(226, 226, 226, 255));
	}
	
QTimeEdit::down-button:pressed, QSpinBox::down-button:pressed, QDateTimeEdit::down-button:pressed {
	 background-color: #ffffff;
	}

