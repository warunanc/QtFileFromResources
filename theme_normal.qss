/*=================== QCheckBox =================*/

QCheckBox {
     spacing: 3px;
	 color: #2F2023;
	padding:2px 2px 2px 2px;
 }
QCheckBox::indicator {
	width: 12px;
	height: 12px;
}
QCheckBox::indicator:unchecked 
{
	background-color: #FFFFFF;
	/*
	border-radius: 3px;	
	*/
	border:1px solid black;
	
}
QCheckBox::indicator:unchecked:hover {}
QCheckBox::indicator:unchecked:pressed { }

QCheckBox::indicator:checked {
	/*background-color: #3F9BF2;*/
/*
	border-radius: 3px;
	border:1px solid #B1B1B1;
*/
	border:1px solid black;
	image: url(:/images/check.ico);
}

QCheckBox::indicator:checked:hover { }
QCheckBox::indicator:checked:pressed { }
QCheckBox::indicator:indeterminate:hover { }
QCheckBox::indicator:indeterminate:pressed {}

/*=================== QRadioButton =================*/
QRadioButton::indicator {
    width: 13px;
    height: 13px;
}

QRadioButton::indicator::unchecked {}
QRadioButton::indicator:unchecked:hover {}
QRadioButton::indicator:unchecked:pressed {}
QRadioButton::indicator::checked {}
QRadioButton::indicator:checked:hover {}
QRadioButton::indicator:checked:pressed {}
QRadioButton::indicator{
	width: 18px;
	height: 18px;
	border-radius: 10px;
}
QRadioButton::indicator:unchecked{
background-color: #FFFFFF;
border:1px solid #B1B1B1;
}
QRadioButton::indicator:checked{
background-color: #3875D7;
border:1px solid #3F9BF2;
image: url(:/images/raidoButtonCheck.ico);
}
QRadioButton::indicator:disabled{}
QRadioButton::indicator:checked:disabled{}
/*================================ QPushButton ================================*/
QPushButton{
	border-radius:3px;
	background-color:#FFFFFF;
	border: 1px solid #C8C8C8;
	padding:7px 10px 7px 10px;
}
QPushButton:pressed {
	background-color:#6C6C6C;
	color:#FFFFFF;
}
QPushButton:checked{
	background-color:#6C6C6C
}
QPushButton:hover{
	border: 1px solid #76ABD7;
}
QPushButton:disabled{
	color: #f2f2f2;
}



/*------------------------MacOKButton---------------*/
.MacOKButton{
	border-radius:3px;
	border: 1px solid #C8C8C8;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(97, 177, 244, 255), stop:1 rgba(0, 133, 243, 255));
	color:#FFFFFF;
	padding:7px 10px 7px 10px;
}
.MacOKButton:hover{	
background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(97, 177, 244, 255), stop:1 rgba(0, 133, 243, 255));
color:#FFFFFF;
}
.MacOKButton:pressed {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0, 133, 243, 255), stop:1 rgba(97, 177, 244, 255));
	font-weight: bold;
}
.MacOKButton:focus{	
background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(97, 177, 244, 255), stop:1 rgba(0, 133, 243, 255));
color:#FFFFFF;
}
.MacOKButton:disabled{	
color: #B5B5B5;
}

/*================================================ QScrollBar ================================================*/
QScrollBar:vertical {
	border:none;
    margin: 18px 0 18px 0; /*remove top and bottom margin if you need to remove arrows*/
    max-width: 11px;
	background:#FCFCFC;
}
QScrollBar::handle:vertical { /*handler(Gradient replaces the background image so If you want add an image to the handler remove gradient)*/
	min-height: 50px;
	border:1px solid #C3C3C3;
	background-repeat:norepeat;
	background-position: center center;
	background-color: #C3C3C3;
	/*
	background-image: url(:/NeonGlow/Themes/NeonGlow/VerticalSBImage.png);
	background: QLinearGradient( x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #484848, stop: 1 #3c3b3b);(to make the background gradient look use this)*/
}

QScrollBar::add-line:vertical {/*bottom arrow container*/
    border: none;
    height: 19px;
	background-image: url(:/images/Scrollbar_vertical_down_Arrow.png);
	background-repeat:no-repeat;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {/*Top arrow container*/
    border: none;
    height: 19px;
	background-image: url(:/images/Scrollbar_vertical_up_Arrow.png);
	background-repeat:no-repeat;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {/*Arrow Point*/
    width: 5px;
    height: 5px;
	background-color: rgba(255, 255, 255, 0);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {/*Handler Back color*/
   background-color: #F9F9F9;
}
/*---------------------------------------------------------------------------------------------------------------------------------------*/
QScrollBar:horizontal  {
  	border:1px solid #E4E4E4;
    margin: 0px 18px 0px 18px;/*remove left and right margin if you need to remove arrows*/
    max-height: 11px;
	background:#FCFCFC;
}
QScrollBar::handle:horizontal  {/*Handler*/
	min-width: 50px;
	border:1px solid #C3C3C3;
	border-radius: 8px;
	/*radius: 8px;*/
	background-color: #C3C3C3;
	/*
	background-image: url(:/NeonGlow/Themes/NeonGlow/HorizontalSBImage.png);
	background-repeat:norepeat;
	background-position: center center;
	background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #484848, stop: 1 #3c3b3b);*/
}
QScrollBar::add-line:horizontal  { /*left arrow container*/
    border: none;
	width: 19px;
    background-image: url(:/images/Scrollbar_Horizontal_left_Arrow.png);
	background-repeat:no-repeat;
	subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal  { /*right arrow container*/
    border: none;
	width: 19px;
    background-image: url(:/images/Scrollbar_Horizontal_right_Arrow.png);
	background-repeat:no-repeat;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
	width: 5px;
    height: 5px;
    background-color: black;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {/*Handler Back color*/
	background-color: #F9F9F9;
}
